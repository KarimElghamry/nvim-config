{
    "suggest.noselect": false,
    "prettier.tabWidth": 4,
    "coc.preferences.formatOnSaveFiletypes": [
        "javascript",
        "typescript",
        "typescriptreact",
        "json",
        "javascriptreact",
        "typescript.tsx",
        "graphql",
        "dart",
        "cpp",
        "css"
    ],
    "explorer.width": 30,
    "explorer.previewAction.onHover": false,
    "explorer.icon.enableNerdfont": true,
    "explorer.keyMappings": {
        "<cr>": ["expandable?", "expand", "open"],
        "v": "open:vsplit"
    }
}
